{
  "name": "expect-axios-matchers",
  "version": "0.0.0-development",
  "description": "expect matchers for axios response",
  "keywords": ["test", "unittest", "integration test", "mocha", "vitest", "expect"],
  "files": ["src", "types", "all.js", "jest.d.ts"],
  "main": "src/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "test": "concurrently -n w: npm:test:*",
    "test:unit": "node -r ./test/setup.js --test-concurrency 1 --experimental-test-snapshots --test",
    "unit:test:update-snapshot": "node -r ./test/setup.js --test-concurrency 1 --experimental-test-snapshots --test-update-snapshots --test",
    "test:types:expect": "( cd types-test/expect-all-type-tests && npm test )",
    "test:types:jest:all": "( cd types-test/jest-all-type-tests && npm test )",
    "test:types:jest:partial": "( cd types-test/jest-partial-type-tests && npm test )",
    "test:types:vitest": "( cd types-test/vitest-type-tests && npm test )",
    "check": "npx @biomejs/biome check",
    "check:fix": "npx @biomejs/biome check --write",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20",
    "axios": "^1.7.3",
    "concurrently": "^8.2.2",
    "expect": "^29.7.0",
    "fastify": "^4.28.1",
    "husky": "^8.0.3",
    "pretty-ansi": "^2.0.0",
    "semantic-release": "^19.0.5"
  },
  "peerDependencies": {
    "expect": ">= 21"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": ["master"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tamir-M/expect-axios-matchers.git"
  },
  "bugs": {
    "url": "https://github.com/Tamir-M/expect-axios-matchers/issues"
  },
  "license": "MIT"
}
